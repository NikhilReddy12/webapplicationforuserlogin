<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>newserv</servlet-name>
        <servlet-class>Newserv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logindao</servlet-name>
        <servlet-class>LoginDao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>welcum</servlet-name>
        <servlet-class>welcum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forgot</servlet-name>
        <servlet-class>forgot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forgtdao</servlet-name>
        <servlet-class>forgtdao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forgtt</servlet-name>
        <servlet-class>forgtt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>newserv</servlet-name>
        <url-pattern>/newserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logindao</servlet-name>
        <url-pattern>/logindao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>welcum</servlet-name>
        <url-pattern>/welcum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forgot</servlet-name>
        <url-pattern>/forgot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forgtdao</servlet-name>
        <url-pattern>/forgtdao</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>signin.jsp</welcome-file>
    </welcome-file-list>
</web-app>
